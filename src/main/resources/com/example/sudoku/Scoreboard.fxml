<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sudoku.ScoreboardController">
   <children>
       <ListView fx:id="listView" layoutX="20.0" layoutY="20.0" prefHeight="500.0" prefWidth="360.0" />
      <Button layoutX="66.0" layoutY="550.0" mnemonicParsing="false" onAction="#onResetClicked" prefHeight="36.0" prefWidth="93.0" text="Reset" textFill="#692a14">
         <font>
            <Font name="Brush Script MT Italic" size="18.0" />
         </font>
      </Button>
      <Button layoutX="244.0" layoutY="550.0" mnemonicParsing="false" onAction="#onBackClicked" prefHeight="36.0" prefWidth="73.0" text="Back">
         <font>
            <Font name="Brush Script MT Italic" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
